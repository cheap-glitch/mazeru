  "main": "./build/index.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:minify",
    "build:minify": "terser --compress --mangle toplevel --comments '/^!/' --output build/index.min.js -- build/index.js",
    "build:ts": "tsc",
    "coverage:collect": "jest --collectCoverage",
    "coverage:upload": "coveralls < .coverage/lcov.info",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@cheap-glitch/typescript",
    "env": {
      "es6": true
    }
  },
  "jest": {
    "preset": "ts-jest",
    "coverageDirectory": ".coverage"
  },
  "devDependencies": {
    "@cheap-glitch/eslint-config-typescript": "^1.0.3",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.19",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "terser": "^5.5.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "type-fest": "^0.8.1",
    "typescript": "^4.1.3"
